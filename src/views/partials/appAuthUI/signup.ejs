
    <div class="main">
        <div class="cover">
            <div class="loader"></div>
          </div>
        <section class="signup">
            <div class="size">
                <div class="signup-content">
                    <div class="signup-form">
                        <h2 class="form-title">Sign up</h2>
                        <form method="POST"  class="register-form" action="javascript-void(0)">
                            <div class="form-group">
                                <label for="name"><i class="zmdi zmdi-account material-icons-name"></i></label>
                                <input type="text" name="name" class="username" placeholder="Username">
                            </div>
                            <div class="form-group">
                                <label for="email"><i class="zmdi zmdi-email"></i></label>
                                <input type="email" name="email" id="email" placeholder="Your Email">
                            </div>
                            <div class="form-group">
                                <label for="pass"><i class="zmdi zmdi-lock"></i></label>
                                <input type="password" class="pass1"  placeholder="Password">
                            </div>
                            <div class="form-group">
                                <label for="re-pass"><i class="zmdi zmdi-lock-outline"></i></label>
                                <input type="password" name="re_pass" class="re_typepass"  placeholder="Repeat your password">
                            </div>
                            <div class="form-group">
                                <input type="checkbox" class="agree-term">
                                <label for="agree-term" class="label-agree-term">
                                    <span></span>I agree all statements in  <a href="/terms" class="term-service">Terms of service</a>
                                </label>
                            </div>
                            <div class="form-group form-button">
                                <input type="submit" class="form-submit" value="Sign up">
                            </div>
                        </form>
                        <div class="social-login">
                            <span class="social-label">Or login with</span>
                            <ul class="socials">
                                <li><a href="/auth/github"><i class="fab fa-github"></i></a></li>
                                <div class="g-signin2" data-onsuccess="onSignin"></div>
                                <li>
                                    <a href="/auth/google">
                                        <!-- <i class="fab fa-google"></i> -->
                                    </a>
                                </li>
                                <div class="data">
                                    <p>Profile Details</p>
                                    <img id="pic" class="img-circle" width="100" height="100">
                                    <p id="email" class="alert alert-danger"></p>
                                </div>
                                
                                
                                
                            </ul>
                        </div>
                    </div>
                    
                    <div class="signup-image">
                        <figure>
                            <img src="/images/auth/singup/signup-image.jpg" alt="sing up image">
                        </figure>
                        <a href="/signin" class="signup-image-link">I am already member</a>
                    </div>
                </div>
            </div>
        </section>
    </div>